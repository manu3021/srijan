#include<stdio.h>
#include<conio.h>
#define MAX_ROW 20
#define MAX_COL 20

void main(void)
{
int a[MAX_ROW][MAX_COL];
int i,j,m,n;
clrscr();
printf("enter the size of array as m*n");
scanf("%d%d",&m,&n);
if((m > MAX_ROW)||(n > MAX_COL))
{
printf("input array size is more than declared");
}
printf("enter %d elements of the array row wise\n", m*n);
for(i=0;i<m;i++)
for(j=0;j<n;j++)
scanf("%d",&a[i][j]);
getch();
}
