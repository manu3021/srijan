#include<stdio.h>
#include<conio.h>
void main(void)
{
int n,x;
int factorial(int k);
clrscr();
printf("Enter the number");
scanf("%d",&n);
x=factorial(n);
printf("Factorial is : %d",x);
getch();
}
int factorial(int k)
{
int product=1,i;
for(i=2;i<=k;i++)
product*=i;
return product;
}